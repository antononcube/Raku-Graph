use v6.d;
use Test;

use Graph;

subtest {
    ok Graph.new();
    is Graph.new().vertex-list, Empty;

    ok Graph.new(<1 2>, []);
    is Graph.new(<1 2>, []).vertex-list, <1 2>;

    ok Graph.new(vertexes => ["1", "2"], edges => []);
    is Graph.new(vertexes => ["1", "2"], edges => []).vertex-list, <1 2>;
}, "has vertexes and no edges graph";


done-testing;
